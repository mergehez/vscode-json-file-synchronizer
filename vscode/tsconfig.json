{
	"compilerOptions": {
		"module": "commonjs",
		"target": "es6",
		"outDir": "out",
		"lib": ["es6", "dom"],
		"sourceMap": true,
		"rootDir": "src",
		"strict": true,
		"noUnusedLocals": false,
		"noUncheckedIndexedAccess": false,
		"noUnusedParameters": false
	},
	"exclude": [
		"node_modules",
		"**/node_modules",
		".vscode-test",
        "vue"
	]
}
